{
  "expo": {
    "name": "ProjectRaid",
    "scheme": "ProjectRaid",
    "slug": "ProjectRaid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/Logo.ico",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/Logo.ico",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/image/Logo.png"
    },

    "expo": {
      "assetBundlePatterns": ["**/*"]
    },
    "plugins": ["expo-router"]
  }
}
